%section.row.well#partners
  .container
    .row
      %h1.text-center.page-header
        Partners
    .row
      #partnercloud
        %canvas#partnercloudcanvas{:width => 500, :height => 500 }
          - @partners.each do |s|
            %li
              = link_to(s.url, title: s.name) do
                = image_tag(s.logo_url)